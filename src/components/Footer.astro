---
import Social from "../components/Social.astro";
const socialInfo = [
  { platform: "github", username: "lemux-one" },
  { platform: "twitter", username: "lemux_one" },
];
---

<style>
  footer {
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>

<footer>
  {
    socialInfo.map((item) => {
      return <Social platform={item.platform} username={item.username} />;
    })
  }
</footer>
